									<label><h5>Información personal</h5></label>
								</legend><br>
							<table width="100%">
								<tr>
									<td width="48%">
										<label>
											<span>Usuario</span>
											<input type="text" name="username" id="username" maxlength="30" required>
											</label>
									</td>
									<td>
									</td>
									<td width="48%">
										<label>
											<span>Correo electrónico</span>
											<input type="email" name="email" id="email" maxlength = "320" required>
											</label>
									</td>
								</tr>
								<tr>
									<td width="48%">
										<label>
											<span>Nombre</span>
											<input type="text" name="name" id="name" maxlength="30" required>
											</label>
									</td>
									<td>
									</td>
									<td width="48%">
										<label>
											<span>Primer apellido</span>
											<input type="text" name="firstLastname" id="firstLastname" maxlength="30" required>
											</label>
									</td>
								</tr>
								<tr>
									<td width="48%">
										<label>
											<span>Segundo apellido</span>
											<input type="text" name="secondLastname" id="secondLastname" maxlength="30">
											</label>
									</td>
									<td>
									</td>
									<td width="48%">
										<label><span>Sexo</span></label><br><br>
											<label>
											<input type="radio" name="sex" id="sexMan" value="M" checked>
											&nbsp;Masculino
											</label>&nbsp;&nbsp;&nbsp;
											<label>
											<input type="radio" name="sex" id="sexWoman" value="W">
											&nbsp;Femenino
											</label>
									</td>
								</tr>
								<tr>
									<td width="48%">
										<label>
											<span>Fecha de nacimiento</span>
											<input type="date" name="birthday" id="birthday" required>
											</label>
									</td>
									<td>
									</td>
									<td width="48%">
										<label>
											<span>Número telefónico</span>
											<input type="text" name="telephoneNumber" id="telephoneNumber" maxlength="15" required>
